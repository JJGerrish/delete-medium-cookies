{
    "manifest_version": 2,

    "name": "Delete Medium Cookies Extension",
    "description": "This extension will delete medium.com cookies in one click",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "browsingData",
        "cookies",
        "https://*/",
        "http://*/"
    ]
}